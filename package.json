{
		"name": "react-app",
		"jest": {
				"moduleNameMapper": {
						"^image![a-zA-Z0-9$_-]+$": "GlobalImageStub",
						"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/imageMock.js",
						"\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
				},
				"setupFilesAfterEnv": [
						"<rootDir>/test_setup.js"
				],
				"testPathIgnorePatterns": [
						"/node_modules/"
				],
				"collectCoverage": true,
				"verbose": true,
				"unmockedModulePathPatterns": [
						"<rootDir>/node_modules/react/",
						"<rootDir>/node_modules/react-dom/",
						"<rootDir>/node_modules/react-addons-test-utils/",
						"<rootDir>/node_modules/fbjs",
						"<rootDir>/node_modules/core-js"
				]
		},
		"version": "1.0.0",
		"description": "",
		"main": "index.js",
		"scripts": {
				"dev": "webpack-dev-server --open --config webpack.dev.js",
				"prod": "webpack --progress --config webpack.prod.js",
				"test": "jest",
				"cypress:open": "cypress open"
		},
		"author": "kate_satsura",
		"license": "ISC",
		"dependencies": {
				"express": "^4.16.4",
				"react": "^16.8.4",
				"react-dom": "^16.8.4",
				"react-redux": "^6.0.1",
				"react-router": "^4.3.1",
				"redux": "^4.0.1"
		},
		"devDependencies": {
				"@babel/core": "^7.3.4",
				"@babel/preset-env": "^7.3.4",
				"@babel/preset-react": "^7.0.0",
				"babel-eslint": "^10.0.1",
				"babel-jest": "^24.5.0",
				"babel-loader": "^8.0.5",
				"babel-preset-es2015": "^6.24.1",
				"babel-preset-react": "^6.24.1",
				"css-loader": "^2.1.1",
				"cypress": "^3.2.0",
				"enzyme": "^3.9.0",
				"enzyme-adapter-react-16": "^1.12.0",
				"extract-text-webpack-plugin": "^4.0.0-beta.0",
				"file-loader": "^3.0.1",
				"html-webpack-plugin": "^3.2.0",
				"jest": "^24.5.0",
				"react-test-renderer": "^16.8.4",
				"style-loader": "^0.23.1",
				"terser-webpack-plugin": "^1.2.3",
				"url-loader": "^1.1.2",
				"webpack": "^4.29.6",
				"webpack-cli": "^3.2.3",
				"webpack-dev-server": "^3.2.1",
				"webpack-merge": "^4.2.1"
		}
}
